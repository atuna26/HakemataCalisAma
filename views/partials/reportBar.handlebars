<div class="uk-child-width-1-3@s uk-grid-match uk-margin-top uk-margin-medium-right" style="margin: 0;width:100vw;"
        uk-grid>
        <div>
            <a href="/fikstur/refereeSearch">
                <div class="uk-card uk-card-secondary uk-card-hover uk-card-body">
                    <h3 class="uk-card-title">Hakem <img src="/img/referee.svg" width="30px" height="30px"
                            style="float:right;opacity:0.5;"></h3>
                    <p>Hakem Sayısı:{{refereeCount}}</p>
                </div>
            </a>
        </div>
        <div>
            <div class="uk-card uk-card-default uk-card-hover uk-card-body">
                <h3 class="uk-card-title">Lig <img src="/img/ball.svg" width="30px" height="30px" style="float:right;">
                </h3>
                <p>Lig Sayısı:{{leagueCount}} </p>
            </div>
        </div>
        <div>
            <a href="/fikstur">
                <div class="uk-card uk-card-hover uk-card-body uk-light" style="background-color: #32d296;">
                    <h3 class="uk-card-title">Fikstür<img src="/img/ball.svg" width="30px" height="30px"
                            style="float:right;"></h3>
                    <p>Oluşturulan Maç Sayısı:{{fixtureCount}}</p>
                </div>
            </a>
        </div>
    </div>
    <div class="hr" style="margin-top: 2em;"></div>